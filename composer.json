{
    "name": "meowphp/routing",
    "description": "Extendable router",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/meowphp/routing",
    "autoload": {
        "psr-4": {
            "Meow\\Routing\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Meow\\Routing\\Test\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "May Meow",
            "email": "hello@mitsuha.dev"
        }
    ],
    "require": {
        "php": ">=8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^1.1",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "suggest": {
        "maymeow/cake-attribute-routing": ">=v0.2.0"
    },
    "support": {
        "issues": "https://github.com/meowphp/routing/issues"
    },
    "scripts": {
        "test": "phpunit tests"
    }
}
